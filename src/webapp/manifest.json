{
	"_version": "1.12.0",
	"sap.app": {
		"id": "Team2",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}, 
		"dataSources": {
			"mainService": {
				"uri": "/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "Team2.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Team2.i18n.i18n"
				}
			},
			"warehouse": {
				"dataSource": "mainService"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "Team2.view",
				"controlId": "app",
				"transition": "slide",
				"controlAggregation": "pages",
				 "bypassed": {
	               "target": "notFound"
	            },
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "overviewpage",
					"target": "overview"
				},
				{
					"pattern": "materials",
					"name": "manageMaterial",
					"target": "materials"
				},
				{
					"pattern": "stocks",
					"name": "manageStock",
					"target": "stocks"
				},
				{

					"pattern": "",
					"name": "addMaterial",
					"target": "add"

					"pattern": "addM",
					"name": "addMaterial",
					"target": "addM"

				}
			],
			"targets": {
				"overview": {
					"viewId": "overviewpage",
					"viewName": "OverviewPage",
					"viewLevel" : 1
				},
				"materials": {
					"viewId": "managematerial",
					"viewName": "ManageMaterial",
					"viewLevel" : 2
				},
				"stocks": {
					"viewId": "managestock",
					"viewName": "ManageStock",
					"viewLevel" : 2
				},

				"add":{
					"viewId": "addmaterial",
					"viewName": "AddMaterial",
					"viewLevel": 2

				"addM": {
					"viewId": "addMaterial",
					"viewName": "AddMaterial",
					"viewLevel" : 3

				},
	            "notFound": {
	               "viewId": "notFound",
	               "viewName": "NotFound",
	               "transition": "show"
	            }
			}
		}
	}
}
